{
  "name": "@simpleryo/web-components",
  "version": "0.1.0",
  "description": "simpleryo web components",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prelink-packages": "yarn install",
    "link-packages": "lerna bootstrap",
    "build": "lerna run build --stream --parallel",
    "lint": "lerna run lint --stream --parallel",
    "lint:fix": "lerna run lint:fix --stream --parallel",
    "start": "lerna run start --stream",
    "start:mock": "lerna run start:mock --stream",
    "start:dev": "lerna run start:dev --stream --parallel",
    "pretest": "rimraf ./packages/**/__tests__/reports",
    "test": "lerna run test --stream --parallel",
    "prepublish-packages": "yarn build",
    "publish-packages": "sh ./npm_publish.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "eslint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simpleryo/web-components.git"
  },
  "author": "SimplerYO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simpleryo/web-components.git/issues"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "1.18.2",
    "classnames": "^2.2.5",
    "contentful": "^5.0.5",
    "cross-env": "^3.1.3",
    "express": "^4.16.2",
    "lodash": "^4.17.4",
    "moment": "^2.22.0",
    "prop-types": "^15.6.1",
    "qrcode.react": "^0.7.2",
    "query-string": "^5.0.1",
    "ramda": "^0.24.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-intl": "2.4.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-scroll": "^1.7.9",
    "react-transition-group": "^2.3.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-nodejs6": "^1.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "child-process-promise": "^2.2.1",
    "colors": "^1.1.2",
    "css-loader": "^1.0.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.6.1",
    "file-loader": "^1.1.6",
    "fs-promise": "^2.0.3",
